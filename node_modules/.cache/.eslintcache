type === \"list\" ? \"headerContainer listMode\" : \"headerContainer\"\n        }\n      >\n        <div className=\"headerList\">\n          <div className=\"headerListItem active\">\n            <FontAwesomeIcon icon={faBed} />\n            <span>Stays</span>\n          </div>\n          <div className=\"headerListItem\">\n            <FontAwesomeIcon icon={faPlane} />\n            <span>Flights</span>\n          </div>\n          <div className=\"headerListItem\">\n            <FontAwesomeIcon icon={faCar} />\n            <span>Car rentals</span>\n          </div>\n          <div className=\"headerListItem\">\n            <FontAwesomeIcon icon={faBed} />\n            <span>Attractions</span>\n          </div>\n          <div className=\"headerListItem\">\n            <FontAwesomeIcon icon={faTaxi} />\n            <span>Airport taxis</span>\n          </div>\n        </div>\n        {type !== \"list\" && (\n          <>\n            <h1 className=\"headerTitle\">\n              A lifetime of discounts? It's Genius.\n            </h1>\n            <p className=\"headerDesc\">\n              Get rewarded for your travels â€“ unlock instant savings of 10% or\n              more with a free Lamabooking account\n            </p>\n            <button className=\"headerBtn\">Sign in / Register</button>\n            <div className=\"headerSearch\">\n              <div className=\"headerSearchItem\">\n                <FontAwesomeIcon icon={faBed} className=\"headerIcon\" />\n                <input\n                  type=\"text\"\n                  placeholder=\"Where are you going?\"\n                  className=\"headerSearchInput\"\n                  onChange={(e) => setDestination(e.target.value)}\n                />\n              </div>\n              <div className=\"headerSearchItem\">\n                <FontAwesomeIcon icon={faCalendarDays} className=\"headerIcon\" />\n                <span\n                  onClick={() => setOpenDate(!openDate)}\n                  className=\"headerSearchText\"\n                >{`${format(dates[0].startDate, \"MM/dd/yyyy\")} to ${format(\n                  dates[0].endDate,\n                  \"MM/dd/yyyy\"\n                )}`}</span>\n                {openDate && (\n                  <DateRange\n                    editableDateInputs={true}\n                    onChange={(item) => setDates([item.selection])}\n                    moveRangeOnFirstSelection={false}\n                    ranges={dates}\n                    className=\"date\"\n                    minDate={new Date()}\n                  />\n                )}\n              </div>\n              <div className=\"headerSearchItem\">\n                <FontAwesomeIcon icon={faPerson} className=\"headerIcon\" />\n                <span\n                  onClick={() => setOpenOptions(!openOptions)}\n                  className=\"headerSearchText\"\n                >{`${options.adult} adult Â· ${options.children} children Â· ${options.room} room`}</span>\n                {openOptions && (\n                  <div className=\"options\">\n                    <div className=\"optionItem\">\n                      <span className=\"optionText\">Adult</span>\n                      <div className=\"optionCounter\">\n                        <button\n                          disabled={options.adult <= 1}\n                          className=\"optionCounterButton\"\n                          onClick={() => handleOption(\"adult\", \"d\")}\n                        >\n                          -\n                        </button>\n                        <span className=\"optionCounterNumber\">\n                          {options.adult}\n                        </span>\n                        <button\n                          className=\"optionCounterButton\"\n                          onClick={() => handleOption(\"adult\", \"i\")}\n                        >\n                          +\n                        </button>\n                      </div>\n                    </div>\n                    <div className=\"optionItem\">\n                      <span className=\"optionText\">Children</span>\n                      <div className=\"optionCounter\">\n                        <button\n                          disabled={options.children <= 0}\n                          className=\"optionCounterButton\"\n                          onClick={() => handleOption(\"children\", \"d\")}\n                        >\n                          -\n                        </button>\n                        <span className=\"optionCounterNumber\">\n                          {options.children}\n                        </span>\n                        <button\n                          className=\"optionCounterButton\"\n                          onClick={() => handleOption(\"children\", \"i\")}\n                        >\n                          +\n                        </button>\n                      </div>\n                    </div>\n                    <div className=\"optionItem\">\n                      <span className=\"optionText\">Room</span>\n                      <div className=\"optionCounter\">\n                        <button\n                          disabled={options.room <= 1}\n                          className=\"optionCounterButton\"\n                          onClick={() => handleOption(\"room\", \"d\")}\n                        >\n                          -\n                        </button>\n                        <span className=\"optionCounterNumber\">\n                          {options.room}\n                        </span>\n                        <button\n                          className=\"optionCounterButton\"\n                          onClick={() => handleOption(\"room\", \"i\")}\n                        >\n                          +\n                        </button>\n                      </div>\n                    </div>\n                  </div>\n                )}\n              </div>\n              <div className=\"headerSearchItem\">\n                <button className=\"headerBtn\" onClick={handleSearch}>\n                  Search\n                </button>\n              </div>\n            </div>\n          </>\n        )}\n      </div>\n    </div>\n  );\n};\n\nexport default Header;\n",[],"/home/vats/Web Dev/booking/client/src/components/navbar/Navbar.jsx",[],[],"/home/vats/Web Dev/booking/client/src/components/mailList/MailList.jsx",[],[],"/home/vats/Web Dev/booking/client/src/components/propertyList/PropertyList.jsx",["105","106"],[],"/home/vats/Web Dev/booking/client/src/components/searchItem/SearchItem.jsx",[],[],"/home/vats/Web Dev/booking/client/src/hooks/useFetch.js",[],[],"/home/vats/Web Dev/booking/client/src/context/searchContext.js",["107"],[],"/home/vats/Web Dev/booking/client/src/context/AuthContexxt.js",[],[],"/home/vats/Web Dev/booking/client/src/pages/Login/Login.jsx",[],[],"/home/vats/Web Dev/booking/client/src/components/Reserve/Reserve.jsx",["108","109","110"],[],{"ruleId":"111","severity":1,"message":"112","line":28,"column":15,"nodeType":"113","messageId":"114","endLine":28,"endColumn":22},{"ruleId":"111","severity":1,"message":"115","line":28,"column":23,"nodeType":"113","messageId":"114","endLine":28,"endColumn":28},{"ruleId":"111","severity":1,"message":"116","line":5,"column":10,"nodeType":"113","messageId":"114","endLine":5,"endColumn":20},{"ruleId":"111","severity":1,"message":"117","line":10,"column":10,"nodeType":"113","messageId":"114","endLine":10,"endColumn":23},{"ruleId":"111","severity":1,"message":"118","line":14,"column":23,"nodeType":"113","messageId":"114","endLine":14,"endColumn":37},{"ruleId":"111","severity":1,"message":"119","line":18,"column":19,"nodeType":"113","messageId":"114","endLine":18,"endColumn":29},{"ruleId":"111","severity":1,"message":"115","line":24,"column":23,"nodeType":"113","messageId":"114","endLine":24,"endColumn":28},{"ruleId":"111","severity":1,"message":"115","li